{
  "extends": [
    "tslint:latest",
    "tslint-config-prettier"
  ],
  "rules": {
    "no-console": false,
    "object-literal-sort-keys": false,
    "quotemark": [
      true,
      "single",
      "avoid-escape"
    ],
    "semicolon": true,
    "no-reference": false,
    "prefer-for-of": false,
    "no-implicit-dependencies": [
      false,
      "dev"
    ]
    // "ordered-imports": false
  }
}
